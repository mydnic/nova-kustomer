{
    "name": "mydnic/nova-kustomer",
    "description": "Laravel Nova Kustomer Feedback Tool",
    "keywords": [
        "laravel",
        "nova",
        "kustomer",
        "feedback",
        "mydnic"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Mydnic\\NovaKustomer\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mydnic\\NovaKustomer\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
